{
	"name": "cyberui",
	"version": "0.1.0-alpha",
	"description": "A react-based component package inspired by the Cyberpunk 2077 universe",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"scripts": {
		"prepare": "husky",
		"format": "prettier --config ./.prettierrc \"src/**/*.{ts,tsx,scss}\" --write",
		"assets-fonts": "mkdirp ./lib/dist/fonts && cp -vruf ./src/fonts/*.woff2 ./lib/dist/fonts && cp -vruf ./src/fonts/*.woff ./lib/dist/fonts",
		"assets": "rm -rf ./lib/dist/ && mkdirp ./lib/dist && npm run assets-fonts",
		"build": "npm run compile && npm run assets && npm run sass",
		"compile": "tsc",
		"clean": "rm -rf lib/",
		"lint": "eslint --ext js,jsx,ts,tsx src --fix",
		"sass": "sass ./src/cyberui.scss ./lib/dist/cyberui.min.css --style=expanded",
		"sass-dev": "sass ./src/cyberui.scss ./lib/dist/cyberui.css",
		"storybook": "storybook dev -p 6006",
		"test": "echo \"Error: no test specified\" && exit 0",
		"build-storybook": "storybook build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/evjero/cyberui.git"
	},
	"keywords": [
		"cyberpunk",
		"ui",
		"react",
		"components"
	],
	"author": "evjero",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/evjero/cyberui/issues"
	},
	"homepage": "https://github.com/evjero/cyberui#readme",
	"devDependencies": {
		"@babel/core": "7.26.10",
		"@babel/plugin-transform-modules-commonjs": "7.26.3",
		"@babel/plugin-transform-typescript": "7.27.0",
		"@babel/preset-env": "^7.26.9",
		"@babel/preset-react": "^7.26.3",
		"@babel/preset-typescript": "^7.27.0",
		"@eslint/compat": "^1.2.8",
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.25.0",
		"@storybook/addon-controls": "8.6.12",
		"@storybook/addon-essentials": "8.6.12",
		"@storybook/addon-interactions": "^8.6.12",
		"@storybook/addon-links": "^8.6.12",
		"@storybook/blocks": "^8.6.12",
		"@storybook/builder-vite": "^8.6.12",
		"@storybook/react": "8.6.12",
		"@storybook/react-vite": "^8.6.12",
		"@storybook/react-webpack5": "^8.6.12",
		"@storybook/testing-library": "^0.2.2",
		"@storybook/theming": "8.6.12",
		"@types/node": "^22.14.1",
		"@types/react": "^19.1.2",
		"@types/react-dom": "^19.1.2",
		"@typescript-eslint/eslint-plugin": "8.30.1",
		"@typescript-eslint/parser": "8.30.1",
		"css-loader": "7.1.2",
		"eslint": "9.25.0",
		"eslint-plugin-jest-dom": "^5.5.0",
		"eslint-plugin-react": "7.37.5",
		"eslint-plugin-react-hooks": "5.2.0",
		"eslint-plugin-storybook": "^0.12.0",
		"husky": "^9.1.7",
		"mkdirp": "^3.0.1",
		"pretty-quick": "4.1.1",
		"prop-types": "^15.8.1",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"sass": "^1.86.3",
		"sass-loader": "16.0.5",
		"storybook": "^8.6.12",
		"style-loader": "4.0.0",
		"typescript": "5.8.3"
	},
	"peerDependencies": {
		"react": "^19.0.0",
		"react-dom": "^19.0.0"
	},
	"dependencies": {
		"classnames": "^2.5.1",
		"react-feather": "^2.0.10"
	}
}
